package com.OHRM;

import org.openqa.selenium.chrome.ChromeDriver;

public class orangehrmApplication2 {



	public static void main(String[] args) {
		// TODO Auto-generated method stub
		ChromeDriver driver;
		 String applicationurladdress="http://127.0.0.1/orangehrm-4.2.0.1/symfony/web/index.php/auth/login";
		 
		System.setProperty("webdriver.chrome.driver","./webdriverfile/chromedriver.exe");
		driver =new ChromeDriver();
	driver.get(applicationurladdress);
	//validation of loginpage 
	 String expectedOrangeHRMTitle="OrangeHRM";
	 System.out.println("the expected orangehrmapplicationtitle loginpage is :-"+expectedOrangeHRMTitle);
	 String actualOrangeHRMTitle ="OrangeHRM";
	System.out.println("the actual orangehrm title of login page is ;-"+actualOrangeHRMTitle);
	if(actualOrangeHRMTitle.equals(actualOrangeHRMTitle)) {
		System.out.println("orangehrmapplicationloginpage tiele matched-PASS");
	}
	else
	{
System.out.println("orangehrmapplicationloginpage tiele matched-PASS");
	}
		
		
			
	}
}

		